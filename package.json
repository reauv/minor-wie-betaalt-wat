{
  "name": "redux-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for webpack, redux & react",
  "scripts": {
    "start": "rm -rf ./build & npm run dev:server & npm run dev:hot",
    "dev:server": "supervisor -w server.js,.env -- --use_strict server.js",
    "dev:hot": "supervisor -w hot.server.js,.env hot.server.js",
    "production": "rm -rf ./build && webpack -p --config webpack.production.config.js",
    "lint": "eslint src"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Robert van Steen <studio@rovansteen.nl> (http://github.com/reauv)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.5.1",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.9.0",
    "nodemon": "^1.9.1",
    "postcss-loader": "^0.8.1",
    "postcss-modules-values": "^1.1.1",
    "postcss-nested": "^1.0.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.1.0",
    "redux-devtools-filter-actions": "^1.1.0",
    "style-loader": "^0.13.0",
    "supervisor": "^0.9.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.1",
    "cookie_js": "^1.2.0",
    "dotenv": "^2.0.0",
    "es6-promise": "^3.2.1",
    "es6-template": "^1.0.4",
    "exports-loader": "^0.6.3",
    "fetch-plus": "^3.10.1",
    "html-loader": "^0.4.3",
    "imports-loader": "^0.6.5",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.4.0",
    "query-string": "^4.1.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.2.1",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.2.1",
    "redux-act": "0.4.0",
    "redux-form": "^4.2.0",
    "redux-thunk": "^1.0.3",
    "webpack-isomorphic-tools": "^2.2.31",
    "whatwg-fetch": "^1.0.0"
  }
}
